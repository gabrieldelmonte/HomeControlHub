{
  "name": "src",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "build:ui": "cd ui && vite build",
    "dev:ui": "cd ui && vite",
    "preview:ui": "cd ui && vite preview",
    "dev": "concurrently \"npm run dev:ui\" \"tsx main.ts\"",
    "start": "npm run build && concurrently \"npm run preview:ui\" \"node dist/main.js\"",
    "prisma": "prisma",
    "migrate:dev": "prisma migrate dev --schema=./db/prisma/schema.prisma --name",
    "generate": "prisma generate --schema=./db/prisma/schema.prisma",
    "seed": "prisma db seed --schema=./db/prisma/schema.prisma",
    "studio": "prisma studio --schema=./db/prisma/schema.prisma",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "prisma": {
    "seed": "node dist/db/seed.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/styled-components": "^5.1.34",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "mqtt": "^5.12.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.28.0",
    "styled-components": "^6.1.18",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ws": "^8.18.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.1",
    "@types/mqtt": "^0.0.34",
    "@types/node": "^22.15.15",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react": "^4.3.3",
    "concurrently": "^9.1.0",
    "prisma": "^5.22.0",
    "prisma-erd-generator": "^2.0.4",
    "ts-node": "^10.9.2",
